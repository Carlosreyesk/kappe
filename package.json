{
  "name": "kappe",
  "version": "1.0.0",
  "description": "psd",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass --include-path scss scss/main.scss public/css/main.css",
    "watch-css": "rm -rf public/css/main.css | nodemon -e scss -x \"npm run build-css\"",
